<?xml version='1.0' encoding='utf-8'?>
<widget id="co.uk.jobooz.helicopter" version="1.3.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Helicopter</name>
    <description>
        Helicopter The Game
    </description>
    <author email="ry_pasley@hotmail.co.uk" href="http://www.rp-digital.co.uk">
        Ryan Pasley
    </author>
    <preference name="phonegap-version" value="cli-6.3.0" />
    <preference name="orientation" value="portrait" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="detect-data-types" value="false" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="SplashScreenDelay" value="10000" />
    <preference name="disable-cursor" value="true" />
    <preference name="android-minSdkVersion" value="9" />
    <preference name="android-installLocation" value="auto" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="webviewbounce" value="false" />
    <preference name="permissions" value="none" />
    <preference name="SuppressesLongPressGesture" value="true" />
    <preference name="Suppress3DTouch" value="true" />
    <allow-navigation href="*" />
    <allow-intent href="*" />
    <icon src="icon.png" />
    <icon gap:density="ldpi" gap:platform="android" height="36" src="images/icons/icon-36-ldpi.png" width="36" />
    <icon gap:density="mdpi" gap:platform="android" height="48" src="images/icons/icon-48-mdpi.png" width="48" />
    <icon gap:density="hdpi" gap:platform="android" height="72" src="images/icons/icon-72-hdpi.png" width="72" />
    <icon gap:density="xhdpi" gap:platform="android" height="96" src="images/icons/icon-96-xhdpi.png" width="96" />
    <icon gap:density="xxhdpi" gap:platform="android" height="144" src="images/icons/icon-144-xxhdpi.png" width="144" />
    <icon gap:density="xxxhdpi" gap:platform="android" height="192" src="images/icons/icon-192-xxxhdpi.png" width="192" />
    <icon gap:platform="ios" height="57" src="images/icons/icon-57.png" width="57" />
    <icon gap:platform="ios" height="72" src="images/icons/icon-72.png" width="72" />
    <icon gap:platform="ios" height="114" src="images/icons/icon-114.png" width="114" />
    <icon gap:platform="ios" height="144" src="images/icons/icon-144.png" width="144" />
    <gap:splash src="splash.png" />
    <gap:splash gap:density="ldpi" gap:platform="android" src="images/screens/splash-200x320-ldpi.png" />
    <gap:splash gap:density="mdpi" gap:platform="android" src="images/screens/splash-320x480-mdpi.png" />
    <gap:splash gap:density="hdpi" gap:platform="android" src="images/screens/splash-480x800-hdpi.png" />
    <gap:splash gap:density="xhdpi" gap:platform="android" src="images/screens/splash-720x1280-xhdpi.png" />
    <gap:splash gap:density="xxhdpi" gap:platform="android" src="images/screens/splash-960x1600-xxhdpi.png" />
    <gap:splash gap:density="xxxhdpi" gap:platform="android" src="images/screens/splash-1280x1920-xxxhdpi.png" />
    <gap:splash gap:platform="ios" height="480" src="images/screens/splash-320x480.png" width="320" />
    <gap:splash gap:platform="ios" height="960" src="images/screens/splash-640x960.png" width="640" />
    <gap:splash gap:platform="ios" height="1136" src="images/screens/splash-640x1136.png" width="640" />
    <gap:splash gap:platform="ios" height="1024" src="images/screens/splash-ipad-portrait.png" width="768" />
    <access origin="*" />
</widget>
